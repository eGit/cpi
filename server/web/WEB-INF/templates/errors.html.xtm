<html xmlns="http://www.w3.org/1999/xhtml">
  <head>
    <title>Gap Data Error</title>
    <link rel="stylesheet" type="text/css" href="http://cpi.cognitiveprofile.com/top.css" />
    <link rel="icon" href="http://cpi.cognitiveprofile.com/favicon.ico" />
  </head>
  <body>
    <div class="logon">{{>logon}}
    </div>
    <div id="first">
      {{#error}}
      <div class="block inside">
        <h1 class="errors"> {{=error_status}}</h1>
        <p class="errors"> {{=error_message}}
        </p>
        <pre class="errors">
{{=error_exception}}
        </pre>
      </div>
      {{/error}}
    </div>
  </body>
</html>
