package cpi.groups;
path /projects;
import oso.data.Person;

/**
 * Name is unique for lookups, random identifier is 
 * the public parameter key.
 * 
 * Accept name for logon member of scope.
 * 
 * Accept identifier for OAuth access.
 */
class Project version 1 {
    *unique String name;
    String identifier;
    Person owner;
    Date created;
}
